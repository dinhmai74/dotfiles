41a54716ebe992515bd6d481b941eb84|file:///Users/dinhmai/workspace/project/end-game/Bowie/app/screens/add-event-screen/add-detail-screen/PickTagContent.tsx|1|1|v|typescriptreact
	import { Spinner } from '@ui-kitten/components'
	import { useGetHotTagsQuery } from 'app-graphql'
	import { AppButton, AppText, Box, Flex, Layout } from 'components'
	import React from 'react'
	import { Color } from 'themes/color'
	
	type PickTagContentProps = {}
	
	const TrendTagColor: Color = 'color-gray-500'
	
	// eslint-disable-next-line @typescript-eslint/no-unused-vars
	export const PickTagContent = (props: PickTagContentProps) => {
	  const { data: hotTags, loading: loadingHotTags } = useGetHotTagsQuery()
	  const renderHotTags = () => {
	    return (
	      <>
	        <Flex>
	          {hotTags?.getTopTenHotTag?.map((v) => (
	            <AppButton variant="bordered" textColor={TrendTagColor} borderColor={TrendTagColor}>
	              {v.name}
	            </AppButton>
	          ))}
	        </Flex>
	      </>
	    )
	  }
	  return (
	    <Layout p="2" px="4">
	      <AppText tx="addNewEventScreen.info.pickTag" variant="h3" padding="0" />
	      <Box>
	        <AppText tx="addNewEventScreen.info.trendingTags" />
	        {loadingHotTags ? <Spinner /> : renderHotTags()}
	      </Box>
	      
	    </Layout>
	  )
	}

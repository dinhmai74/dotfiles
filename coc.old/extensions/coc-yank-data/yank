	import * as React from 'react'
	import { Story, StoryScreen, UseCase } from '../../../storybook/views'
	import { AppText } from './AppText'
	
	declare let module
	
	const variants: any[] = [
	  'transparent',
	  'h2Medium',
	  'h2',
	  'h3',
	  'h3Medium',
	  'p',
	  'p2',
	  's2',
	  'buttonprimary',
	  'buttonbasic',
	  'buttonlink',
	  'buttonghost',
	  'buttonoutlined',
	]
	
